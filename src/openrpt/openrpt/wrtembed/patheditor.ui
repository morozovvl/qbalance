 <class>PathEditor</class>
 <widget class="QDialog" name="PathEditor">
  <property name="geometry">
   <rect>
    <x>0</x>
    <y>0</y>
    <width>248</width>
    <height>161</height>
   </rect>
  </property>
  <property name="windowTitle">
   <string>Stroke properties</string>
  </property>
  <layout class="QHBoxLayout">
   <property name="margin">
    <number>11</number>
   </property>
   <item>
    <layout class="QVBoxLayout">
     <item>
      <layout class="QHBoxLayout">
       <item>
        <widget class="QLabel" name="_lblWidth">
         <property name="text">
          <string>Width:</string>
         </property>
        </widget>
       </item>
       <item>
        <widget class="QSpinBox" name="_sbWidth"/>
       </item>
      </layout>
     </item>
     <item>
      <widget class="QGroupBox" name="_gbStyle">
       <property name="title">
        <string>Style</string>
       </property>
       <layout class="QGridLayout">
        <property name="margin">
         <number>11</number>
        </property>
        <item row="0" column="0">
         <widget class="QRadioButton" name="_rbSolidStyle">
          <property name="text">
           <string/>
          </property>
          <property name="icon">
           <iconset resource="../images/OpenRPTWrtembed.qrc">
            <normaloff>:/OpenRPT/images/line_solid.png</normaloff>:/OpenRPT/images/line_solid.png</iconset>
          </property>
          <property name="iconSize">
           <size>
            <width>80</width>
            <height>16</height>
           </size>
          </property>
         </widget>
        </item>
        <item row="1" column="0">
         <widget class="QRadioButton" name="_rbDashedStyle">
          <property name="text">
           <string/>
          </property>
          <property name="icon">
           <iconset resource="../images/OpenRPTWrtembed.qrc">
            <normaloff>:/OpenRPT/images/line_dashed.png</normaloff>:/OpenRPT/images/line_dashed.png</iconset>
          </property>
          <property name="iconSize">
           <size>
            <width>80</width>
            <height>16</height>
           </size>
          </property>
         </widget>
        </item>
        <item row="2" column="0">
         <widget class="QRadioButton" name="_rbDottedStyle">
          <property name="text">
           <string/>
          </property>
          <property name="icon">
           <iconset resource="../images/OpenRPTWrtembed.qrc">
            <normaloff>:/OpenRPT/images/line_dotted.png</normaloff>:/OpenRPT/images/line_dotted.png</iconset>
          </property>
          <property name="iconSize">
           <size>
            <width>80</width>
            <height>16</height>
           </size>
          </property>
         </widget>
        </item>
       </layout>
      </widget>
     </item>
    </layout>
   </item>
   <item>
    <layout class="QVBoxLayout">
     <item>
      <widget class="QPushButton" name="_btnAccept">
       <property name="text">
        <string>&amp;OK</string>
       </property>
      </widget>
     </item>
     <item>
      <widget class="QPushButton" name="_btnCancel">
       <property name="text">
        <string>&amp;Cancel</string>
       </property>
      </widget>
     </item>
     <item>
      <spacer>
       <property name="orientation">
        <enum>Qt::Vertical</enum>
       </property>
       <property name="sizeType">
        <enum>QSizePolicy::Expanding</enum>
       </property>
       <property name="sizeHint" stdset="0">
        <size>
         <width>20</width>
         <height>20</height>
        </size>
       </property>
      </spacer>
     </item>
     <item>
      <widget class="QPushButton" name="_btnColor">
       <property name="text">
        <string>Color...</string>
       </property>
      </widget>
     </item>
    </layout>
   </item>
  </layout>
 </widget>
 <layoutdefault spacing="6" margin="11"/>
 <tabstops>
  <tabstop>_btnColor</tabstop>
  <tabstop>_btnAccept</tabstop>
  <tabstop>_btnCancel</tabstop>
 </tabstops>
 <resources>
  <include location="../images/OpenRPTWrtembed.qrc"/>
 </resources>
 <connections>
  <connection>
   <sender>_btnAccept</sender>
   <signal>clicked()</signal>
   <receiver>PathEditor</receiver>
   <slot>accept()</slot>
   <hints>
    <hint type="sourcelabel">
     <x>20</x>
     <y>20</y>
    </hint>
    <hint type="destinationlabel">
     <x>20</x>
     <y>20</y>
    </hint>
   </hints>
  </connection>
  <connection>
   <sender>_btnCancel</sender>
   <signal>clicked()</signal>
   <receiver>PathEditor</receiver>
   <slot>reject()</slot>
   <hints>
    <hint type="sourcelabel">
     <x>20</x>
     <y>20</y>
    </hint>
    <hint type="destinationlabel">
     <x>20</x>
     <y>20</y>
    </hint>
   </hints>
  </connection>
 </connections>
</ui>
